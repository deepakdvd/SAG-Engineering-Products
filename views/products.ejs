<script>
  var catelist=[];
  function categorysort(data){
    
    if($(data).prop("checked") == true){
      var temp=$(data).val();
      catelist.push(temp);
      
    }else{
      var temp1=$(data).val();
      var ind=catelist.indexOf(temp1)
      catelist.splice(ind,1);
      
    }
  }

  function shoecatedata(){
    if(catelist.length>0){
      $('#myModal').modal('hide');

    $.ajax({
           type: 'POST',
          url: '/selectedcate?data='+catelist,
          success: function(data) {
            
            $('#datashow').empty();
            for(var i=0;i<data.length;i++){
            $('#datashow').append('<div class="mbr-gallery-item mbr-gallery-item--p2" data-video-url="false" data-tags="Awesome">\
                  <div href="#lb-gallery2-h" data-slide-to="0" data-toggle="modal">\
                    <img src="'+data[i].image.imn+'" alt="image">\
                    <a href="/single-product?id='+data[i]._id+'">\
                      <span class="icon-focus"></span>\
                      <span class="mbr-gallery-title mbr-fonts-style display-7">'+data[i].Title+'</span>\
                    </a>\
                  </div>\
                </div>');
              }
          }
        });
      }else{
        alert('please select alteast one category')
        }
     
  }

  function searchtitle(title){
   
    if(title.length>3){
    $.ajax({
           type: 'POST',
          url: '/searchtitle?data='+title,
          success: function(data) {
            $('#datashow').empty();
            for(var i=0;i<data.length;i++){
            $('#datashow').append('<div class="mbr-gallery-item mbr-gallery-item--p2" data-video-url="false" data-tags="Awesome">\
                  <div href="#lb-gallery2-h" data-slide-to="0" data-toggle="modal">\
                    <img src="'+data[i].image.imn+'" alt="image">\
                    <a href="/single-product?id='+data[i]._id+'">\
                      <span class="icon-focus"></span>\
                      <span class="mbr-gallery-title mbr-fonts-style display-7">'+data[i].Title+'</span>\
                    </a>\
                  </div>\
                </div>');
              }
          }
        });
      }
  }
</script>